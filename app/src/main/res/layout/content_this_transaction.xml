<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ThisTransaction"
    tools:showIn="@layout/activity_this_transaction">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:paddingStart="0dp"
        android:paddingEnd="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="5dp"
            android:paddingEnd="10dp">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="103dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/PAN"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

            <TextView
                android:id="@+id/pan"
                android:layout_width="244dp"
                android:layout_height="25dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

            <TextView
                android:id="@+id/transtype"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:textAlignment="center"
                android:textColor="@color/note_list_text"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/responseMsg"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:textColor="@color/note_list_text"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/dashes"
                android:textAlignment="center"
                android:textColor="@color/note_list_text"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/responseMsg" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="72dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/expiry"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <TextView
                android:id="@+id/expiryDate"
                android:layout_width="263dp"
                android:layout_height="25dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pan" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="323dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:text="@string/dashes"
                android:textAlignment="center"
                android:textColor="@color/note_list_text"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.517"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/transtype" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="74dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/rrn"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/rrnText"
                android:layout_width="252dp"
                android:layout_height="0dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/expiryDate" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="110dp"
                android:layout_height="24dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/name"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView17" />

            <TextView
                android:id="@+id/cardHolderName"
                android:layout_width="233dp"
                android:layout_height="26dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/rrnText" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/amountCap"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView25" />

            <TextView
                android:id="@+id/amountText_2"
                android:layout_width="264dp"
                android:layout_height="24dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardType" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="79dp"
                android:layout_height="25dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/cardType"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView19" />

            <TextView
                android:id="@+id/cardType"
                android:layout_width="249dp"
                android:layout_height="27dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardHolderName" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="85dp"
                android:layout_height="21dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:text="@string/dateTime"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <TextView
                android:id="@+id/transDateTime"
                android:layout_width="236dp"
                android:layout_height="27dp"
                android:layout_marginTop="12dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/amountText_2" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="92dp"
                android:layout_height="21dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="20dp"
                android:text="@string/origin_app"
                android:textColor="@color/note_list_text"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/originAppText"
                android:layout_width="242dp"
                android:layout_height="19dp"
                android:layout_marginTop="16dp"
                android:textAlignment="viewEnd"
                android:textColor="@color/note_list_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/transDateTime" />

        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>